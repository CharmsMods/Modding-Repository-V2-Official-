{
	"name": "Venge.io Resource Swapper",
	"version": "0.0.3",
	"manifest_version": 2,
	"description": "https://github.com/Classy08",
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"*://*.venge.io/*",
		"http://127.0.0.1:8080/*",
		"*://storage.googleapis.com/*"
	],
	"web_accessible_resources": [
		"*.obj",
		"*.fbx",
		"*.gltf",
		"*.png",
		"*.jpeg",
		"*.jpg",
		"*.mp4",
		"*.mp3",
		"*.css",
		"*.ttf",
		"*.otf",
		"*.ico",
		"*.svg",
		"*.txt",
		"*.glb"
	],
	"background": {
		"scripts": [
			"init.js"
		]
	},
	"browser_action": {
		"default_popup": "popup.html"
	}
}
